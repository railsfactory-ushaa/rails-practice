<table>
  <thead>
    <tr>
      <th>Order Id </th>
      <th>Card Type</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
<% @order.each do |f| %>
      <tr>
        <td><%= f.id %></td>
        <td><%= f.card_type %></td>
        <td><%= f.order_status.status %></td>
      </tr>
 <% end %>
</tbody>
</table>
