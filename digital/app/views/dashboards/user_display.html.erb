<h1>Listing users</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Contact Number</th>
      <th>Registered Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user.each do |user| %>
      <tr>
        <td><%= user.first_name %></td>
        <td><%= user.email %></td>
        <td><%= user.contact_number %></td>
        <td><%= user.created_at %></td>
    <td><%= link_to 'Orders', {:controller => "dashboards", :action => "user_display_order", :id => user.id } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

