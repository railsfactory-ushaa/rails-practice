<h1>Listing schedules</h1>

<table>
  <thead>
    <tr>
      <th>Device</th>
      <th>User</th>
      <th>Order</th>
      <th>Fromdate</th>
      <th>Todate</th>
      <th>Fromtime</th>
      <th>Totime</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @schedules.each do |schedule| %>
      <tr>
        <td><%= schedule.device_id %></td>
        <td><%= schedule.user_id %></td>
        <td><%= schedule.order_id %></td>
        <td><%= schedule.fromdate %></td>
        <td><%= schedule.todate %></td>
        <td><%= schedule.fromtime %></td>
        <td><%= schedule.totime %></td>
        <td><%= link_to 'Show', schedule %></td>
        <td><%= link_to 'Edit', edit_schedule_path(schedule) %></td>
        <td><%= link_to 'Destroy', schedule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Schedule', new_schedule_path %>
